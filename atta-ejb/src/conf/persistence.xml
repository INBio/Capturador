<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="Enterprise-ejbPU2" transaction-type="JTA">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <jta-data-source>jdbc/_Atta</jta-data-source>
    <properties>
      <property name="hibernate.default_schema" value="atta"/>
      <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect"/>
      <property name="hibernate.show_sql" value="false"/>
      <property name="hibernate.format_sql" value="false"/>
      <property name="org.hibernate.SQL" value="false"/>
      <property name="hibernate.use_sql_comments" value="false"/>
      <property name="hibernate.engine.QueryParameters" value="debug"/>
    </properties>
  </persistence-unit>
</persistence>
